{
    "name": "drupal/oas",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "John",
            "email": "lijd@139.com"
        }
    ],
    "support": {
        "issues": "https://www.drupal.org/project/issues/dsi",
        "source": "http://cgit.drupalcode.org/dsi"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.cn"
        }
    },
    "require": {
        "composer/installers": "~1.0",
        "drupal/core": "^8 || ~9",
        "cweagans/composer-patches": "^1.7",
        "oomphinc/composer-installers-extender": "~1.0",
        "wikimedia/composer-merge-plugin": "^1.4",
        "drupal/drupal-library-installer-plugin": "^0.3",
        "drush/drush": "^10.3",
        "drupal/entity_browser": "^2.5",
        "drupal/console": "^1.9",
        "drupal/migrate_tools": "^5.0",
        "drupal/migrate_source_xls": "1.x-dev",
        "drupal/memcache": "^2.2",
        "drupal/avatars": "^1.1",
        "drupal/components": "2.x-dev",
        "drupal/tb_megamenu": "1.x-dev",
        "drupal/audiofield": "^1.9",
        "npm-asset/dropzone": "^5.7",
        "drupal/webform": "6.0.0-alpha20",
        "drupal/persistent_login": "^1.3",
        "drupal/drush_language": "^1.0@RC",
        "drupal/devel": "^4.0",
        "drupal/block_style_plugins": "^1.4",
        "drupal/entityqueue_form_widget": "^2.0",
        "drupal/slick": "~2.0",
        "bower-asset/blazy": "^1.8",
        "npm-asset/slick-carousel": "^1.8",
        "drupal/calendar": "^1.0@alpha",
        "drupal/fullcalendar_view": "^5.0",
        "kgaut/potx": "dev-8.x-1.x",
        "drupal/token": "^1.7",
        "drupal/token_filter": "^1.2",
        "bower-asset/fullcalendar": "^5.3",
        "drupal/csv_serialization": "2.x-dev",
        "drupal/ludwig": "^1.1",
        "drupal/examples": "^3.0",
        "drupal/field_group": "^3.1",
        "drupal/config_update": "^1.7",
        "drupal/custom_formatters": "^3.0@beta",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/charts": "5.0.x",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/dynamic_entity_reference": "^1.11",
        "drupal/entity": "^1.1",
        "drupal/diff": "^1.0",
        "drupal/feeds": "^3.0@alpha",
        "phpspec/prophecy-phpunit": "^2",
        "drupal/features": "^3.11",
        "drupal/bootstrapper": "5.x-dev",
        "drupal/varnish_purge": "^2.0",
        "drupal/purge": "^3.0",
        "drupal/phpword": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-8.x-1.x": "1.x-dev"
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "core": ["type:drupal-core"],
            "profiles/{$name}": ["type:drupal-profile"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "libraries/slick": ["npm-asset/slick-carousel"],
            "libraries/ace": ["npm-asset/ace-builds"],
            "libraries/jquery-ui-touch-punch": ["bower-asset/jqueryui-touch-punch"],
            "libraries/masonry": ["npm-asset/masonry-layout"],
            "libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "themes/custom/{$name}": ["type:drupal-custom-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-libraries": {
            "library-directory": "libraries",
            "libraries": [
                {"name": "dropzone","package": "npm-asset/dropzone"},
                {"name": "blazy", "package": "npm-asset/blazy"},
                {"name": "slick", "package": "npm-asset/slick-carousel"},
                {"name": "ace", "package": "npm-asset/ace-builds"},
                {"name": "chartjs", "package": "bower-asset/chartjs"},
                {"name": "c3","package": "bower-asset/c3"},
                {"name": "d3", "package": "bower-asset/d3"},
                {"name": "jquery-ui-touch-punch", "package": "bower-asset/jqueryui-touch-punch"},
                {"name": "masonry", "package": "npm-asset/masonry-layout"},
                {"name": "imagesloaded", "package": "npm-asset/imagesloaded"},
                {"name": "swagger-ui", "package": "swagger-api/swagger-ui"}
            ]
        },
        "merge-plugin": {
            "include": [
                "modules/dsi/core/dsi_media/composer.json",
                "modules/dsi/core/dsi_slider_media/composer.json",
                "modules/contrib/charts/modules/charts_highcharts/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        },
        "patches": {
            "drupal/drupal": {
                "Add startup configuration for PHP server": "https://www.drupal.org/files/issues/add_a_startup-1543858-30.patch"
            }
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    }
}
