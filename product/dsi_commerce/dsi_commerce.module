<?php

/**
 * @file
 * Contains dsi_commerce.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function dsi_commerce_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the dsi_commerce module.
    case 'help.page.dsi_commerce':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('The initial commerce in dsi.') . '</p>';
      return $output;

    default:
  }
}
