msgstr ""

#: core/modules/help_topics/help_topics/user.security_account_settings.html.twig:20
msgid "[site:url]: The URL of your web site."
msgstr ""

#: core/modules/help_topics/help_topics/user.security_account_settings.html.twig:21
msgid "[site:login-url]: The URL where users can log in to your site."
msgstr ""

#: core/modules/help_topics/help_topics/user.security_account_settings.html.twig:22
msgid "[user:display-name]: The user's displayed name."
msgstr ""

#: core/modules/help_topics/help_topics/user.security_account_settings.html.twig:23
msgid "[user:account-name]: The users's account name."
msgstr ""

#: core/modules/help_topics/help_topics/user.security_account_settings.html.twig:24
msgid "[user:mail]: The user's email alias."
msgstr ""

#: core/modules/help_topics/help_topics/user.security_account_settings.html.twig:25
msgid "[user:one-time-login-url]: An expiring URL that a user can use to log in once, if they need to reset their password."
msgstr ""

#: core/modules/help_topics/help_topics/user.security_account_settings.html.twig:29
msgid "In the <em>Manage</em> administrative menu, navigate to <em>Configuration</em> &gt; <em>People</em> &gt; <a href=\"@account_settings_url\"><em>Account settings</em></a>."
msgstr ""

#: core/modules/help_topics/help_topics/user.security_account_settings.html.twig:30
