<?php

/**
 * @file
 * Contains dsi_files.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function dsi_files_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the dsi_files module.
    case 'help.page.dsi_files':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('The file plus for dsi.') . '</p>';
      return $output;

    default:
  }
}
