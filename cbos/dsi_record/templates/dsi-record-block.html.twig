{#
/**
* @file
* Default theme implementation to display a block.
*
* @see template_preprocess_block()
*
* @ingroup themeable
*/
#}
{% set classes = [
    'clear-both',
] %}
<div class="vertical-box-column p-r-30 d-none d-lg-table-cell" style="width: 400px">
    <div id="external-events" class="fc-event-list">

        <h5 class="m-t-0 m-b-15"><font style="vertical-align: inherit;"><font
                        style="vertical-align: inherit;">已过期({{ content.expired_data.count }})</font></font></h5>
        {% for value in content.expired_data.data %}

            <div class="fc-event" data-color="#00acac">
                <div class="fc-event-icon">
                    <i class="fas fa-circle fa-fw f-s-9 text-grey" data-toggle="popover"></i>
                </div>
                <font style="vertical-align: inherit;">
                    <font style="vertical-align: inherit;" >
                        {{ value }}
                    </font>
                </font>
            </div>
        {% endfor %}

        <hr class="bg-grey-lighter m-b-15">
        <h5 class="m-t-0 m-b-15"><font style="vertical-align: inherit;"><font
                        style="vertical-align: inherit;">本周({{ content.expired_data.count }})</font></font></h5>
        {% for value in content.expired_data.data %}

            <div class="fc-event" data-color="#b6c2c9">
                <div class='fc-event-icon'>
                    <i class='fas fa-circle fa-fw f-s-9 text-blue'></i>
                </div>
                <font style="vertical-align: inherit;"><font style="vertical-align: inherit;">
                        {{ value }}
                    </font></font>
            </div>
        {% endfor %}
    </div>
    <button
            type="button"
            class="btn btn-success"
            data-toggle="popover"

    >
        右侧的 Popover
    </button>
    <button
            type="button"
            class="btn btn-warning"
            title="Popover title"
            data-container="body"
            data-toggle="popover"
            data-placement="right"
            data-content="右侧的 Popover 中的一些内容">
        右侧的 Popover
    </button>
</div>

<script>

</script>





