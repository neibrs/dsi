{#
/**
* @file
* Default theme implementation to display a block.
*
* @see template_preprocess_block()
*
* @ingroup themeable
*/
#}
<div class="vertical-box-column p-r-30 d-none d-lg-table-cell" style="width: 400px">
  <div class="widget-todolist widget-todolist-rounded m-b-30" data-id="widget">
    <!-- begin widget-todolist-header -->
    <div class="widget-todolist-header">
      <div class="widget-todolist-header-left">
        <h4 class="widget-todolist-header-title">本周</h4>
      </div>
      <div class="widget-todolist-header-right">
        <div class="widget-todolist-header-total"><span>{{ content.expired_data.count }}</span><small>Done</small></div>
      </div>
    </div>
    <!-- end widget-todolist-header -->
    <!-- begin widget-todolist-body -->
    <div class="widget-todolist-body">
      <!-- begin widget-todolist-item -->
      {% for key,value in content.expired_data.data %}
      <div class="widget-todolist-item">
        <div class="widget-todolist-input">
          <div class="checkbox checkbox-css pt-0">
            <input type="checkbox" id="widget_todolist_{{ key+1 }}">
            <label for="widget_todolist_{{ key+1 }}" class="p-l-15">&nbsp;</label>
          </div>
        </div>
        <div class="widget-todolist-content">
          <h4 class="widget-todolist-title"> {{ value }}</h4>
          <p class="widget-todolist-desc">2020/11/11 00:00</p>
        </div>
        <div class="widget-todolist-icon">
          <a href="#"><i class="fa fa-question-circle"></i></a>
        </div>
      </div>
      {% endfor %}
      <!-- end widget-todolist-item -->
    </div>
    <!-- end widget-todolist-body -->
  </div>

  <div class="widget-todolist widget-todolist-rounded m-b-30" data-id="widget">
    <!-- begin widget-todolist-header -->
    <div class="widget-todolist-header">
      <div class="widget-todolist-header-left">
        <h4 class="widget-todolist-header-title">已过期</h4>
      </div>
      <div class="widget-todolist-header-right">
        <div class="widget-todolist-header-total"><span>{{ content.expired_data.count }}</span><small>Done</small></div>
      </div>
    </div>
    <!-- end widget-todolist-header -->
    <!-- begin widget-todolist-body -->
    <div class="widget-todolist-body">
      <!-- begin widget-todolist-item -->
      {% for key,value in content.expired_data.data %}
        <div class="widget-todolist-item">
          <div class="widget-todolist-input">
            <div class="checkbox checkbox-css pt-0">
              <input type="checkbox" id="widget_overdue_{{ key+1 }}">
              <label for="widget_overdue_{{ key+1 }}" class="p-l-15">&nbsp;</label>
            </div>
          </div>
          <div class="widget-todolist-content">
            <h4 class="widget-todolist-title"> {{ value }}</h4>
            <p class="widget-todolist-desc">2020/11/11 00:00</p>
          </div>
          <div class="widget-todolist-icon">
            <a href="#"><i class="fa fa-question-circle"></i></a>
          </div>
        </div>
      {% endfor %}
      <!-- end widget-todolist-item -->
    </div>
    <!-- end widget-todolist-body -->
  </div>
</div>






